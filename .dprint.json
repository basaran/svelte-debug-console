{
  "$schema": "https://dprint.dev/schemas/v0.json",
  "incremental": true,
  "indentWidth": 4,
  "typescript": {
  },
  "json": {
    "indentWidth": 4
  },
  "markdown": {
  },
  "includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json,md,scss,css}"],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json"
  ],

  "plugins": [
    "https://plugins.dprint.dev/typescript-0.49.0.wasm",
    "https://plugins.dprint.dev/json-0.12.1.wasm",
    "https://plugins.dprint.dev/markdown-0.9.2.wasm",
    "https://plugins.dprint.dev/prettier-0.2.2.exe-plugin@63b06beba3acd51e6d23379c47db342a1a24f3d88f3c52a775e2db3426124582"
  ]
}
